{
  "name": "party-everyday-server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test-ci": "npm run test --ci --reporters=default --reporters=jest-junit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.513.0",
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.1",
    "@ngrok/ngrok": "^1.3.0",
    "@types/nodemailer": "^6.4.15",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jest-mock-extended": "^3.0.7",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "typeorm": "^0.3.20",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/services/**/*.(ts|js)",
      "src/modules/**/*.(ts|js)",
      "!**/node_modules/**",
      "!**/src/main.ts",
      "!**/src/common/**",
      "!**/src/config/**",
      "!**/src/entities/**",
      "!**/src/modules/**/*.module.ts",
      "!**/src/services/**/*.module.ts",
      "!**/src/modules/**/*.proxy.module.ts",
      "!**/src/modules/**/*.dto.ts",
      "!**/src/modules/**/*.presenter.ts"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "@modules/(.*)$": "<rootDir>/src/modules/$1",
      "@services/(.*)$": "<rootDir>/src/services/$1",
      "@common/(.*)$": "<rootDir>/src/common/$1",
      "@decorators/(.*)$": "<rootDir>/src/common/decorators/$1",
      "@utils/(.*)$": "<rootDir>/src/common/utils/$1",
      "@config/(.*)$": "<rootDir>/src/config/$1",
      "@entities/(.*)$": "<rootDir>/src/entities/$1",
      "@enums/(.*)$": "<rootDir>/src/common/enums/$1"
    },
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
